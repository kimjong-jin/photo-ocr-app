{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "react-jsx",
    "module": "es2020",
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
